<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

  <RelativeLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:id="@+id/v"
      android:layout_margin="5dp"
      android:layout_marginStart="20dp"
      android:layout_marginEnd="20dp">

      <ImageView
          android:id="@+id/imageView4"
          android:layout_width="38dp"
          android:layout_height="38dp"
          android:layout_marginLeft="10dp"
          android:layout_marginTop="20dp"
          app:srcCompat="@drawable/eth" />

      <TextView
          android:id="@+id/textView143"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginLeft="10dp"
          android:layout_marginTop="20dp"
          android:layout_toRightOf="@id/imageView4"
          android:text="TextView"
          android:textColor="#000" />

      <TextView
          android:id="@+id/textView145"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_below="@id/textView143"
          android:layout_marginLeft="10dp"
          android:textSize="10sp"
          android:layout_marginTop="5dp"
          android:layout_toRightOf="@id/imageView4"
          android:text="TextView"
          />

      <TextView
          android:id="@+id/textView146"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginTop="20dp"
          android:textColor="#000"
          android:layout_marginRight="15dp"
          android:layout_toLeftOf="@id/imageView59"
          android:text="TextView" />

      <TextView
          android:id="@+id/textView147"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_below="@id/textView146"
          android:layout_marginTop="7dp"
          android:layout_marginRight="15dp"
          android:textSize="10sp"
          android:layout_toLeftOf="@id/imageView59"
          android:text="TextView"
          />

      <ImageView
          android:id="@+id/imageView59"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignParentRight="true"
          android:layout_marginTop="40dp"
          android:layout_marginRight="10dp"
          android:src="@drawable/ic_keyboard_arrow_right_green_24dp" />
  </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        android:layout_marginTop="5dp"
        android:layout_margin="5dp"
        android:background="@drawable/divider"
        android:layout_below="@id/v"/>


</RelativeLayout>